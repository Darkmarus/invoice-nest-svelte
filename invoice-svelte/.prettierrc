{
  "plugins": ["prettier-plugin-svelte"],
  "tabWidth": 2,
  "useTabs": false,
  "singleQuote": true,
  "semi": true,
  "bracketSpacing": true,
  "arrowParens": "always",
  "trailingComma": "es5",
  "bracketSameLine": true,
  "printWidth": 120,
  "endOfLine": "lf",
  "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
